<?xml version="1.0" encoding="UTF-8"?>
<cronentries>

  <cron>
    <url>/Notification</url>
    <description>Send reminder messages to users who sign up</description>
    <schedule>every 6 hours synchronized</schedule>
    <timezone>America/Los_Angeles</timezone>
  </cron>
  <cron>
    <url>/Rescue</url>
    <description>Send rescue messages to users who miss assignments</description>
    <schedule>every 3 hours synchronized</schedule>
    <timezone>America/Los_Angeles</timezone>
  </cron>
  <cron>
  	<url>/DataStoreCleaner?Task=CleanAll</url>
  	<description>Provides monthly routine maintenance for datastore entities</description>
  	<schedule>1 of month 01:15</schedule>
    <timezone>America/Los_Angeles</timezone>
   </cron>

</cronentries>